 בשנת 1678 ושנתיים אחר כך, בגיל\r\n        29, היה דוקטור לתאולוגיה. דה לה סאל מת ברואן.\r\n      </span>\r\n    );\r\n    link =\r\n      \"https://he.wikipedia.org/wiki/%D7%96%27%D7%90%D7%9F-%D7%91%D7%98%D7%99%D7%A1%D7%98_%D7%93%D7%94_%D7%9C%D7%94_%D7%A1%D7%90%D7%9C\";\r\n    read = \"לקריאה נוספת\";\r\n  }\r\n\r\n  return (\r\n    <Card className=\"card-cont\">\r\n      <CardImg src={baptiste} className=\"card-image\" />\r\n      <CardBody className=\"card-body\">\r\n        <CardText className=\"card-text\">{text}</CardText>\r\n        <CardLink href={link} className=\"card-link\">\r\n          {read}\r\n        </CardLink>\r\n      </CardBody>\r\n    </Card>\r\n  );\r\n}\r\nexport default MyCard;\r\n","C:\\Users\\salwa\\Desktop\\Github\\school-website\\src\\components\\Modal.js",[],["111","112"],"C:\\Users\\salwa\\Desktop\\Github\\school-website\\src\\components\\Slider.js",[],"C:\\Users\\salwa\\Desktop\\Github\\school-website\\src\\components\\sidemenu\\PdfPage.js",[],"C:\\Users\\salwa\\Desktop\\Github\\school-website\\src\\components\\sidemenu\\Horaires.js",[],"C:\\Users\\salwa\\Desktop\\Github\\school-website\\src\\components\\sidemenu\\Bacheliers.js",[],"C:\\Users\\salwa\\Desktop\\Github\\school-website\\src\\components\\sidemenu\\Result.js",[],{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":26,"column":10,"nodeType":"119","messageId":"120","endLine":26,"endColumn":18},{"ruleId":"117","severity":1,"message":"121","line":31,"column":10,"nodeType":"119","messageId":"120","endLine":31,"endColumn":16},{"ruleId":"117","severity":1,"message":"122","line":1,"column":17,"nodeType":"119","messageId":"120","endLine":1,"endColumn":26},{"ruleId":"117","severity":1,"message":"123","line":1,"column":28,"nodeType":"119","messageId":"120","endLine":1,"endColumn":36},{"ruleId":"117","severity":1,"message":"124","line":3,"column":10,"nodeType":"119","messageId":"120","endLine":3,"endColumn":25},{"rul